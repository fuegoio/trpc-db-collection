{
  "name": "trpc-db-collection",
  "type": "module",
  "version": "0.0.10",
  "description": "tRPC collection for Tanstack DB",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "npm run build",
    "typecheck": "tsc --noEmit",
    "test": "vitest run"
  },
  "keywords": [],
  "author": "Alexis Tacnet <alexistacnet@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fuegoio/trpc-db-collection.git"
  },
  "packageManager": "pnpm@10.13.1",
  "devDependencies": {
    "@types/node": "^24.5.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "zod": "^4.1.12"
  },
  "peerDependencies": {
    "@tanstack/react-db": "^0.1.24",
    "@tanstack/store": "^0.7.5",
    "@trpc/server": "^11.4.3"
  }
}
